<div id="player-details-wrapper">
    <h2>Player Details</h2>
    <p id="profile-hits">{{#if player.hits}}Profile Hits: {{numCommaFormat player.hits}}{{/if}}</p>
</div>

<div id="exp">{{numCommaFormat player.wins}} EXP {{#ifCond change.wins '>' 0}}<span class="change {{#ifCond change.wins '<' 0}}neg{{/ifCond}}">{{#ifCond change.wins '>' 0}}+{{/ifCond}}{{change.wins}}</span>{{/ifCond}}</div>

<div id="season1-trophy-wrapper">
    <div class="trophy-container">
        <div class="trophy">
            <h6>MCCS</h6>
            <i class="fas fa-crown tier-{{trophyTier 'mccs' total_mccs}}"></i>
            <span class="bar">
                <i class='fas fa-crown tier-1 tippy' data-title="1,000 MCCS" style="{{#ifCond player.season5.score '<' 1000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-crown tier-2 tippy' data-title="1,250 MCCS" style="{{#ifCond player.season5.score '<' 1250}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-crown tier-3 tippy' data-title="1,500 MCCS" style="{{#ifCond player.season5.score '<' 1500}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-crown tier-4 tippy' data-title="1,750 MCCS" style="{{#ifCond player.season5.score '<' 1750}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-crown tier-5 tippy' data-title="2,000 MCCS" style="{{#ifCond player.season5.score '<' 2000}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.season5.score '/' 2000) '*' 100 100}}%"></span>
                <span class="progress-number">{{numCommaFormat total_mccs}} / 2,000</span>
            </span>
        </div>

        <!--<div class="trophy">-->
            <!--<h6>RANK 50</h6>-->
            <!--<i class="tier-{{trophyTier '50' total_50s}}">50</i>-->
            <!--<span class="bar">-->
                <!--<i class='tier-1 tippy' data-title="1 RANK 50" style="{{#ifCond total_50s '<' 1}}opacity:0.2;{{/ifCond}}">50</i>-->
                <!--<i class='tier-2 tippy' data-title="2 RANK 50" style="{{#ifCond total_50s '<' 2}}opacity:0.2;{{/ifCond}}">50</i>-->
                <!--<i class='tier-3 tippy' data-title="3 RANK 50" style="{{#ifCond total_50s '<' 3}}opacity:0.2;{{/ifCond}}">50</i>-->
                <!--<i class='tier-4 tippy' data-title="4 RANK 50" style="{{#ifCond total_50s '<' 4}}opacity:0.2;{{/ifCond}}">50</i>-->
                <!--<i class='tier-5 tippy' data-title="5 RANK 50" style="{{#ifCond total_50s '<' 5}}opacity:0.2;{{/ifCond}}">50</i>-->
                <!--<span class="progress" style="width: {{mathMax (math total_50s '/' 5) '*' 100 100}}%"></span>-->
                <!--<span class="progress-number">{{total_50s}} / 5</span>-->
            <!--</span>-->
        <!--</div>-->

        <div class="trophy">
            <h6>MVP</h6>
            <i class="tier-{{trophyTier 'mvp' player.first_place}}">1<sup style="font-size: 12px;">st</sup></i>
            <span class="bar" style="margin: 20px auto;">
                <i class='tier-1 tippy' data-title="500 1<sup>st</sup> Place" style="{{#ifCond player.first_place '<' 500}}opacity:0.2;{{/ifCond}};top:-18px;">1<sup>st</sup></i>
                <i class='tier-2 tippy' data-title="1000 1<sup>st</sup> Place" style="{{#ifCond player.first_place '<' 1000}}opacity:0.2;{{/ifCond}};top:-18px;">1<sup>st</sup></i>
                <i class='tier-3 tippy' data-title="1500 1<sup>st</sup> Place" style="{{#ifCond player.first_place '<' 1500}}opacity:0.2;{{/ifCond}};top:-18px;">1<sup>st</sup></i>
                <i class='tier-4 tippy' data-title="2000 1<sup>st</sup> Place" style="{{#ifCond player.first_place '<' 2000}}opacity:0.2;{{/ifCond}};top:-18px;">1<sup>st</sup></i>
                <i class='tier-5 tippy' data-title="2500 1<sup>st</sup> Place" style="{{#ifCond player.first_place '<' 2500}}opacity:0.2;{{/ifCond}};top:-18px;">1<sup>st</sup></i>
                <span class="progress" style="width: {{mathMax (math player.first_place '/' 2500) '*' 100 100}}%"></span>
                <span class="progress-number">{{player.first_place}} / 2500</span>
            </span>
        </div>

        <div class="trophy">
            <h6>SNIPER</h6>
            <i class="tier-{{trophyTier 'headshot' player.headshots}} fas fa-skull"></i>
            <span class="bar">
                <i class='tier-1 tippy' data-title="12,500 Headshots" style="{{#ifCond player.headshots '<' 12500}}opacity:0.2;{{/ifCond}};top:0;"><i class="fas fa-skull"></i></i>
                <i class='tier-2 tippy' data-title="25,000 Headshots" style="{{#ifCond player.headshots '<' 25000}}opacity:0.2;{{/ifCond}};top:0;"><i class="fas fa-skull"></i></i>
                <i class='tier-3 tippy' data-title="37,500 Headshots" style="{{#ifCond player.headshots '<' 37500}}opacity:0.2;{{/ifCond}};top:0;"><i class="fas fa-skull"></i></i>
                <i class='tier-4 tippy' data-title="50,000 Headshots" style="{{#ifCond player.headshots '<' 50000}}opacity:0.2;{{/ifCond}};top:0;"><i class="fas fa-skull"></i></i>
                <i class='tier-5 tippy' data-title="62,500 Headshots" style="{{#ifCond player.headshots '<' 62500}}opacity:0.2;{{/ifCond}};top:0;"><i class="fas fa-skull"></i></i>
                <span class="progress" style="width: {{mathMax (math player.headshots '/' 62500) '*' 100 100}}%"></span>
                <span class="progress-number">{{player.headshots}} / 62500</span>
            </span>
        </div>
    </div>
    <div class="trophy-container">
        <div class="trophy">
            <h6>WINNER</h6>
            <i class="fas fa-award tier-{{trophyTier 'wins' player.wins}}"></i>
            <span class="bar">
                <i class='fas fa-award tier-1 tippy' data-title="1,500 WINS" style="{{#ifCond player.wins '<' 1500}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-award tier-2 tippy' data-title="3,000 WINS" style="{{#ifCond player.wins '<' 3000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-award tier-3 tippy' data-title="4,500 WINS" style="{{#ifCond player.wins '<' 4500}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-award tier-4 tippy' data-title="6,000 WINS" style="{{#ifCond player.wins '<' 6000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-award tier-5 tippy' data-title="7,500 WINS" style="{{#ifCond player.wins '<' 7500}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.wins '/' 7500) '*' 100 100}}%"></span>
                <span class="progress-number">{{numCommaFormat player.wins}} / 7500</span>
            </span>
        </div>
        <div class="trophy">
            <h6>ATHLETIC</h6>
            <i class="far fa-chart-bar tier-{{trophyTier 'kd' player.kd}}"></i>
            <span class="bar">
                <i class='far fa-chart-bar tier-1 tippy' data-title="1KD" style="{{#ifCond player.kd '<' 1}}opacity:0.2;{{/ifCond}}"></i>
                <i class='far fa-chart-bar tier-2 tippy' data-title="1.25KD" style="{{#ifCond player.kd '<' 1.25}}opacity:0.2;{{/ifCond}}"></i>
                <i class='far fa-chart-bar tier-3 tippy' data-title="1.5KD" style="{{#ifCond player.kd '<' 1.5}}opacity:0.2;{{/ifCond}}"></i>
                <i class='far fa-chart-bar tier-4 tippy' data-title="1.75KD" style="{{#ifCond player.kd '<' 1.75}}opacity:0.2;{{/ifCond}}"></i>
                <i class='far fa-chart-bar tier-5 tippy' data-title="2KD" style="{{#ifCond player.kd '<' 2}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.kd '/' 2) '*' 100 100}}%"></span>
                <span class="progress-number">{{player.kd}}KD / 2KD</span>
            </span>
        </div>
        <div class="trophy">
            <h6>KILLER</h6>
            <i class="fas fa-skull-crossbones tier-{{trophyTier 'kills' player.kills}}"></i>
            <span class="bar">
                <i class='fas fa-skull-crossbones tier-1 tippy' data-title="25,000 KILLS" style="{{#ifCond player.kills '<' 25000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-skull-crossbones tier-2 tippy' data-title="50,000 KILLS" style="{{#ifCond player.kills '<' 50000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-skull-crossbones tier-3 tippy' data-title="75,000 KILLS" style="{{#ifCond player.kills '<' 75000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-skull-crossbones tier-4 tippy' data-title="100,000 KILLS" style="{{#ifCond player.kills '<' 100000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-skull-crossbones tier-5 tippy' data-title="125,000 KILLS" style="{{#ifCond player.kills '<' 125000}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.kills '/' 125000) '*' 100 100}}%"></span>
                <span class="progress-number">{{numCommaFormat player.kills}} / 125,000</span>
            </span>
        </div>
    </div>
    <div class="trophy-container">
        <div class="trophy">
            <h6>MATCHMODER</h6>
            <i class="fas fa-gamepad tier-{{trophyTier 'matches' player.matches}}"></i>
            <span class="bar">
                <i class='fas fa-gamepad tier-1 tippy' data-title="2,000 MATCHES" style="{{#ifCond player.matches '<' 2000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-gamepad tier-2 tippy' data-title="4,000 MATCHES" style="{{#ifCond player.matches '<' 4000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-gamepad tier-3 tippy' data-title="6,000 MATCHES" style="{{#ifCond player.matches '<' 6000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-gamepad tier-4 tippy' data-title="8,000 MATCHES" style="{{#ifCond player.matches '<' 8000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-gamepad tier-5 tippy' data-title="10,000 MATCHES" style="{{#ifCond player.matches '<' 10000}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.matches '/' 10000) '*' 100 100}}%"></span>
                <span class="progress-number">{{numCommaFormat player.matches}} / 10,000</span>
            </span>
        </div>
        <div class="trophy">
            <h6>SCIENTIST</h6>
            <i class="fas fa-exchange-alt tier-{{trophyTier 'wl' player.wl}}"></i>
            <span class="bar">
                <i class='fas fa-exchange-alt tier-1 tippy' data-title="1.5 WL" style="{{#ifCond player.wl '<' 1.5}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-exchange-alt tier-2 tippy' data-title="2.0 WL" style="{{#ifCond player.wl '<' 2}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-exchange-alt tier-3 tippy' data-title="2.5 WL" style="{{#ifCond player.wl '<' 2.5}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-exchange-alt tier-4 tippy' data-title="3.0 WL" style="{{#ifCond player.wl '<' 3}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-exchange-alt tier-5 tippy' data-title="3.5 WL" style="{{#ifCond player.wl '<' 3.5}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.wl '/' 3.5) '*' 100 100}}%"></span>
                <span class="progress-number">{{player.wl}}WL / 3.5WL</span>
            </span>
        </div>
        <div class="trophy">
            <h6>NERD</h6>
            <i class="fas fa-clock tier-{{trophyTier 'playtime' player.epoch}}"></i>
            <span class="bar">
                <i class='fas fa-clock tier-1 tippy' data-title="15 DAYS" style="{{#ifCond player.epoch '<' 1296000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-clock tier-2 tippy' data-title="30 DAYS" style="{{#ifCond player.epoch '<' 2592000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-clock tier-3 tippy' data-title="45 DAYS" style="{{#ifCond player.epoch '<' 3888000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-clock tier-4 tippy' data-title="60 DAYS" style="{{#ifCond player.epoch '<' 5184000}}opacity:0.2;{{/ifCond}}"></i>
                <i class='fas fa-clock tier-5 tippy' data-title="75 DAYS" style="{{#ifCond player.epoch '<' 6480000}}opacity:0.2;{{/ifCond}}"></i>
                <span class="progress" style="width: {{mathMax (math player.epoch '/' 6480000) '*' 100 100}}%"></span>
                <span class="progress-number">{{hoursToDay player.playtime}} / 75d</span>
            </span>
        </div>
    </div>
</div>