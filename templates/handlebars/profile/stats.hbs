<div id="stats-header">
    <h5>Stats</h5>

    <ol>
        <li class="tab" id="all-tab" data-type="all-wrapper"><b>All</b></li>
        <span>|</span>
        <li class="tab" id="s1-tab" data-type="season1-wrapper"><b>Season 1</b></li>
        <span>|</span>
        <li class="tab" id="s2-tab" data-type="season2-wrapper"><b>Season 2</b></li>
        <span>|</span>
        <li class="tab active" id="s3-tab" data-type="season3-wrapper"><b>Season 3</b></li>
    </ol>
</div>


<div class="tab-wrapper active-tab" id="season3-wrapper">
    <div class="stat-container">
        <div class="stat-item" id="score-item">
            <div>MCC Score</div>
            <span>{{numCommaFormat player.season3.score}}</span>
            {{#ifCond change.score '>' 0}}<span class="change {{#ifCond change.score '<' 0}}neg{{/ifCond}}">{{#ifCond change.score '>' 0}}+{{/ifCond}}{{change.score}}</span>{{/ifCond}}
            {{#ifCond leaderboard.s3_score '>' 0}}
                {{#if player.ban}}
                    <div class="percent">NOT RANKED</div>
                {{else}}
                    <div class="percent" style="color: {{percentColors leaderboard.s3_score player_count}}">#{{leaderboard.s3_score}} <span>&#8226;</span> Top {{percent leaderboard.s3_score player_count}}</div>
                {{/if}}
            {{else}}
                <div class="percent">NOT RANKED</div>
            {{/ifCond}}
