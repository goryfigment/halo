                        <label for="ms-2v2-series-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="h3-team-doubles">
                        <input type="checkbox" id="h3-team-doubles-checkbox-{{player__id}}" class="checkbox-input" {{#if v_h3_team_doubles}}checked{{/if}} value="v_h3_team_doubles">
                        <label for="h3-team-doubles-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="halo-reach-team-hardcore">
                        <input type="checkbox" id="halo-reach-team-hardcore-checkbox-{{player__id}}" class="checkbox-input" {{#if v_halo_reach_team_hardcore}}checked{{/if}} value="v_halo_reach_team_hardcore">
                        <label for="halo-reach-team-hardcore-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="halo-reach-invasion">
                        <input type="checkbox" id="halo-reach-invasion-checkbox-{{player__id}}" class="checkbox-input" {{#if v_halo_reach_invasion}}checked{{/if}} value="v_halo_reach_invasion">
                        <label for="halo-reach-invasion-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="h2c-team-hardcore">
                        <input type="checkbox" id="h2c-team-hardcore-checkbox-{{player__id}}" class="checkbox-input" {{#if v_h2c_team_hardcore}}checked{{/if}} value="v_h2c_team_hardcore">
                        <label for="h2c-team-hardcore-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="hce-hardcore-doubles">
                        <input type="checkbox" id="hce-hardcore-doubles-checkbox-{{player__id}}" class="checkbox-input" {{#if v_hce_hardcore_doubles}}checked{{/if}} value="v_hce_hardcore_doubles">
                        <label for="hce-hardcore-doubles-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="hce-team-doubles">
                        <input type="checkbox" id="hce-team-doubles-checkbox-{{player__id}}" class="checkbox-input" {{#if v_hce_team_doubles}}checked{{/if}} value="v_hce_team_doubles">
                        <label for="hce-team-doubles-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="halo-reach-team-slayer">
                        <input type="checkbox" id="halo-reach-team-slayer-checkbox-{{player__id}}" class="checkbox-input" {{#if v_halo_reach_team_slayer}}checked{{/if}} value="v_halo_reach_team_slayer">
                        <label for="halo-reach-team-slayer-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>

