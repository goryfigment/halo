<div>
    <h3>Xbox</h3>
    <table class="table-container" data-type="xbox">
        <thead>
            <tr>
                <th class="sortable">ID</th>
                <th class="sortable">GAMERTAG</th>
                <th>H3 Slayer</th>
                <th>H3 Hardcore</th>
                <th>MS 2v2</th>
                <th>H3 Dubs</th>
                <th>Reach Hardcore</th>
                <th>Reach Invasion</th>
                <th>H2C Hardcore</th>
                <th>HCE Hardcore Dubs</th>
                <th>HCE Dubs</th>
                <th>Reach Slayer</th>
            </tr>
        </thead>

        <tbody>
            {{#each xbox}}
                <tr class="player" data-id="{{player__id}}">
                    <td class="id" data-column="id">{{player__id}}</td>
                    <td class="gamertag" data-column="gamertag">{{player__gamertag}}</td>
                    <td class="h3-team-slayer">
                        <input type="checkbox" id="h3-team-slayer-checkbox-{{player__id}}" class="checkbox-input" {{#if v_h3_team_slayer}}checked{{/if}} value="v_h3_team_slayer">
                        <label for="h3-team-slayer-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="h3-team-hardcore">
                        <input type="checkbox" id="h3-team-hardcore-checkbox-{{player__id}}" class="checkbox-input" {{#if v_h3_team_hardcore}}checked{{/if}} value="v_h3_team_hardcore">
                        <label for="h3-team-hardcore-checkbox-{{player__id}}" class="check-box-wrapper">
                            <span class="check-box">
                                <svg width="12px" height="10px">
                                    <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                </svg>
                            </span>
                            <span class="check-box-label"></span>
                        </label>
                    </td>
                    <td class="ms-2v2-series">
